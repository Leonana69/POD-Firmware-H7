{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Drivers/**",
            ],
            "defines": ["USE_HAL_DRIVER", "STM32H725xx"],
